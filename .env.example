# Apify Configuration
# Get your API token from: https://console.apify.com/account/integrations
APIFY_API_TOKEN=your_apify_api_token_here

# Optional: Apify actor IDs (defaults are provided in code)
# APIFY_WEBSITE_CRAWLER_ID=apify/website-content-crawler
# APIFY_SITEMAP_EXTRACTOR_ID=lukaskrivka/sitemap-sniffer
# APIFY_GOOGLE_PLACES_ID=compass/crawler-google-places

# Supabase Configuration
# Get these from: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Application Configuration
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000
